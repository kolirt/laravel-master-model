{
    "name": "kolirt/laravel-master-model",
    "description": "The package that simplifies working with models, saving relationships, and uploading files in Laravel",
    "type": "library",
    "keywords": [
        "framework",
        "laravel",
        "relations",
        "upload",
        "images",
        "files",
        "auto",
        "fill"
    ],
    "homepage": "https://github.com/kolirt/laravel-master-model",
    "license": "MIT",
    "version": "4.0.9",
    "authors": [
        {
            "name": "kolirt"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=10.0"
    },
    "autoload": {
        "psr-4": {
            "Kolirt\\MasterModel\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kolirt\\MasterModel\\ServiceProvider"
            ]
        }
    }
}
